<h3>Student Form</h3>

<%= form_tag students_path do %>
    <label>First name:</label><br>
    <%= text_field_tag name="student[first_name]" %><br>

    <label>Last name:</label><br>
    <%= text_area_tag name="student[last_name]" %><br>

    <%= submit_tag "Submit Student"%>
<% end %>

<%#= params.inspect %>